{
	"settings": {
		"timeout": 300,
		"logs": {
			"traffic": false,
			"debug": false,
			"filename": "logs/random-qsefew.tsv"
		}
	},
	"scheduler": {
		"type": "simple",
		"settings": {
			"executiontime": -1,
			"iterations": 10,
			"rampupdelay": 20.0,
			"concurrentusers": 1
		}
	},
	"connectionSettings": {
		"server": "<my-server-url>",
		"mode": "jwt",
		"virtualproxy": "jwt",
		"security": true,
		"mocked": false,
		"jwtsettings": {
			"keypath": "mock.pem",
			"claims": "{\"user\":\"{{.UserName}}\",\"directory\":\"{{.Directory}}\"}"
		}
	},
	"loginSettings" : {
        "type": "prefix",
        "settings": {
		  "prefix": "gopher",
		  "directory": "anydir"
        }
	},
	"scenario": [
		{
			"action": "OpenHub",
			"label": "Open the hub"
		},
		{
			"label": "Open a random app",
			"action": "OpenApp",
			"settings": {
				"appmode" : "randomnamefromlist",
				"list": ["<my-app-name>"]
			}
		},  {
			"label": "randomaction",
			"action": "RandomAction",
			"settings": {
				"iterations": 5,
				"actions": [
					{
						"type": "thinktime",
						"weight": 1
					},
					{
						"type": "sheetobjectselection",
						"weight": 3
					},
					{
						"type": "changesheet",
						"weight": 5
					},
					{
						"type": "clearall",
						"weight": 1
					}
				],
				"thinktimesettings": {
					"type": "static",
					"delay": 0.1
				}
			}
		}
	]
}
